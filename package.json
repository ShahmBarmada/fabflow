{
  "name": "fabflow",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "concurrently \"npm:build:*\"",
    "build:remix": "remix build",
    "build:sass": "npm run generate:css -- --style=compressed --no-source-map",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:remix": "cross-env NODE_ENV=development remix dev --manual -c \"node --watch-path server.js --watch server.js\"",
    "dev:sass": "npm run generate:css -- --watch",
    "generate:css": "sass --load-path=node_modules app/:app/",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@carbon/react": "^1.56.0",
    "@prisma/client": "^5.13.0",
    "@remix-run/css-bundle": "^2.9.1",
    "@remix-run/express": "^2.9.1",
    "@remix-run/node": "^2.9.1",
    "@remix-run/react": "^2.9.1",
    "ag-grid-react": "^31.2.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "isbot": "^5.1.5",
    "morgan": "^1.10.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.9.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/source-map-support": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "chokidar": "^3.6.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prisma": "^5.13.0",
    "sass": "^1.75.0",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
